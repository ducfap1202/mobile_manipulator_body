<?xml version="1.0"?>
<launch>
    
    <!-- define arguments -->
    <arg name="world" default="(find mobile_manipulator_body)/worlds/tett.world"/>
    <arg name="paused" default="false"/>
    <arg name="use_sim_time" default="true"/>
    <arg name="gui" default="true"/>
    <arg name="debug" default="false" />
    <arg name="verbose" default="true" />



    <!-- startup simulated WORLD -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find mobile_manipulator_body)/worlds/tett.world"/>
        <arg name="paused" value="$(arg paused)" />
        <arg name="use_sim_time" value="$(arg use_sim_time)" />
        <arg name="gui" value="$(arg gui)" />
        <arg name="debug" value="$(arg debug)" />
        <arg name="verbose" value="$(arg verbose)" />
    </include>

    <!-- Gazebo simulator-->
    <param name="robot_description" textfile="$(find mobile_manipulator_body)/urdf/robot_base.urdf" />
  <node name="spawn_model" pkg="gazebo_ros" type="spawn_model" 
        args="urdf -param robot_description -model mobile_manipulator_body"/>
     <rosparam command="load" file="$(find mobile_manipulator_body)/config/control.yaml" />
  <node name="base_controller_spawner" pkg="controller_manager" type="spawner" args="robot_base_joint_publisher robot_base_velocity_controller"/>   
</launch>